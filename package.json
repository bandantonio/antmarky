{
  "name": "antmarky",
  "version": "1.0.0",
  "description": "Antmarky is a static-site generator for Markdown based on Node.js/EJS",
  "main": "server.js",
  "scripts": {
    "build": "node src/commands/build.js",
    "serve": "node server.js",
    "format": "prettier --check .",
    "lint": "semistandard",
    "test": "nyc --reporter=lcovonly --reporter=text mocha 'tests/**/*.test.js'"
  },
  "semistandard": {
    "ignore": ["tests/"]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.25.0",
    "chai": "^4.3.6",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "joi": "^17.6.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1",
    "semistandard": "^16.0.1",
    "showdown": "^1.9.1"
  }
}
