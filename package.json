{
  "name": "antmarky",
  "version": "0.10.0",
  "description": "Antmarky is a static-site generator for Markdown based on Node.js/EJS",
  "main": "server.js",
  "scripts": {
    "build": "node src/commands/build.js",
    "serve": "node server.js",
    "lint": "semistandard",
    "test": "nyc --reporter=lcovonly --reporter=text mocha 'test/**/*.test.js'",
    "bump": "node bump-version.js",
    "changelog": "auto-changelog --handlebars-setup .changelog/helper.js --template .changelog/changelog-template.hbs --commit-limit false"
  },
  "semistandard": {
    "ignore": [
      "test/"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "axios": "^0.25.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "joi": "^17.6.0",
    "mocha": "^9.2.2",
    "mock-fs": "^5.1.2",
    "nyc": "^15.1.0",
    "semistandard": "^16.0.1",
    "showdown": "^1.9.1"
  }
}
